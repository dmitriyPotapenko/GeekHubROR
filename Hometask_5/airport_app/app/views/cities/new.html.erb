<h1> New City </h1>

<%= form_with scope: :city, url: country_cities_path, local: true do |f| %>
	<%= 'Name:' %><br>
	<%= f.text_field :name %><br><br>
	  <%= 'Select City Emblem:' %>
  <%= f.file_field :avatar %><br><br>
	<%= f.submit %>
<% end %>
<br><%= link_to 'Back', country_path(params[:country_id]) %>