<h1><%= 'Edit ' %>
<%=  @employee.second_name %>
<%= 'Info' %></h1>

<%= link_to 'Employees', admin_position_employees_path(:position_id) %>
<%= ' | ' %>
<%= link_to 'Home Page', root_path %><br><br>
<%= form_with(model: @employee, url: admin_employee_path, method: :patch, local: true) do |f| %>
<%= 'First Name:' %><br><br>
<%= f.text_field :first_name %><br><br>
<%= 'Second Name:' %><br><br>
<%= f.text_field :second_name %><br><br>
<%= 'Select New Employee Photo:' %>
<%= f.file_field :avatar %><br><br>
<%= f.submit %>
<% end %>